{
  "name": "omhh-cli",
  "description": "cli utility for pushing data to omhh-cms",
  "version": "0.0.1",
  "author": "Ryan Sheehan @ryansheehan",
  "private": true,
  "bin": {
    "omhh": "./bin/run"
  },
  "bugs": "https://github.com/ryansheehan/omhh-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "cli-ux": "^5.5.1",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.27.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^11.0.3",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "oclif": "^1.18.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ryansheehan/omhh-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "omhh",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/ryansheehan/omhh-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "oclif-dev": "oclif-dev",
    "oclif": "oclif",
    "build": "oclif-dev pack",
    "start": "dotenv ./bin/run"
  },
  "types": "lib/index.d.ts"
}
