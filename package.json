{
  "name": "omhh-cli",
  "description": "cli utility for pushing data to omhh-cms",
  "version": "0.0.1",
  "author": "Ryan Sheehan @ryansheehan",
  "private": true,
  "bin": {
    "omhh": "./bin/run"
  },
  "bugs": "https://github.com/ryansheehan/omhh-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "sqlite": "^4.0.21",
    "sqlite3": "^5.0.2",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.59",
    "chai": "^4.3.4",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "oclif": "^1.17.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ryansheehan/omhh-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "omhh",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/ryansheehan/omhh-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "oclif-dev": "oclif-dev",
    "oclif": "oclif",
    "build": "oclif-dev pack",
    "start": "dotenv ./bin/run"
  },
  "types": "lib/index.d.ts"
}
